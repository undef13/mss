{
    "identifier": "bs_roformer",
    "model_type": "bs_roformer",
    "stft": {
        "n_fft": 2048,
        "hop_length": 512,
        "win_length": 2048,
        "conv_dtype": "float16"
    },
    "model": {
        "chunk_size": 588800,
        "output_stem_names": [
            "bass",
            "drums",
            "other",
            "vocals",
            "guitar",
            "piano"
        ],
        "dim": 256,
        "depth": 12,
        "rms_norm_eps": 5.960464477539063e-08,
        "rotary_embed_dtype": "float16",
        "transformer_residual_dtype": "float32"
    },
    "audio_io": {
        "target_sample_rate": 44100,
        "force_channels": 2
    },
    "inference": {
        "normalize_input_audio": true,
        "batch_size": 4,
        "force_weights_dtype": "float16",
        "use_autocast_dtype": "float16"
    },
    "chunking": {
        "method": "overlap_add_windowed",
        "overlap_ratio": 0.5,
        "window_shape": "hann"
    },
    "masking": {
        "add_sub_dtype": "float32",
        "out_dtype": "float16"
    },
    "derived_stems": {
        "instrumental": {
            "operation": "subtract",
            "stem_name": "mixture",
            "by_stem_name": "vocals"
        },
        "drums_and_bass": {
            "operation": "sum",
            "stem_names": [
                "drums",
                "bass"
            ]
        }
    },
    "output": {
        "stem_names": "all",
        "file_format": "flac"
    }
}