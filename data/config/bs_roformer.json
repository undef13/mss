{
    "identifier": "bs_roformer",
    "model_type": "bs_roformer",
    "model": {
        "chunk_size": 588800,
        "output_stem_names": [
            "bass",
            "drums",
            "other",
            "vocals",
            "guitar",
            "piano"
        ],
        "dim": 256,
        "depth": 12,
        "stereo": true,
        "time_transformer_depth": 1,
        "freq_transformer_depth": 1,
        "stft_n_fft": 2048,
        "stft_hop_length": 512
    },
    "audio_io": {
        "target_sample_rate": 44100,
        "force_channels": 2
    },
    "inference": {
        "normalize_input_audio": true,
        "batch_size": 4,
        "compute_dtype": "float16"
    },
    "chunking": {
        "method": "overlap_add_windowed",
        "overlap_ratio": 0.5,
        "window_shape": "hann"
    },
    "derived_stems": {
        "instrumental": {
            "operation": "subtract",
            "stem_name": "mixture",
            "by_stem_name": "vocals"
        },
        "drums_and_bass": {
            "operation": "sum",
            "stem_names": [
                "drums",
                "bass"
            ]
        }
    },
    "output": {
        "stem_names": "all",
        "file_format": "flac",
        "bit_depth": 24
    }
}